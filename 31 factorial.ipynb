    "def fact(x):\n",
    "\n",
    "    result = 1\n",
    "    if x==1 or x==0:\n",
    "        print(f'the x factorial is equal to 1')\n",
    "        \n",
    "    else:\n",
    "        for i in range(x):\n",
    "            result=result*(i+1)\n",
    "            \n",
    "        return result"
   ]
